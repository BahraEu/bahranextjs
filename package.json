        "next-compose-plugins": "^2.2.1",
        "next-i18next": "^4.0.1",
        "next-routes": "^1.4.2",
        "next-seo": "^4.17.0",
        "prop-types": "^15.7.2",
        "query-string": "^6.13.8",
