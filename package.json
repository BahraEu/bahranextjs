        "apollo-link-schema": "1.2.5",
        "apollo-server-express": "^2.19.2",
        "axios": "^0.21.1",
        "classnames": "^2.2.6",
        "compression": "^1.7.4",
        "cookie": "^0.4.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
