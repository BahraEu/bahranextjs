{
    "name": "bahra",
    "version": "0.1.0",
    "author": "",
    "license": "ISC",
    "scripts": {
        "bahra": "next",
        "codegen": "graphql-codegen --config codegen.yml",
        "dev": "nodemon",
        "build": "next build && tsc --project tsconfig.server.json && next export",
        "start": "cross-env NODE_ENV=production node dist/server/index.js",
        "build:staging": "NODE_ENV=production CUSTOM_ENV=staging next build && npm run in",
        "build:local": "NODE_ENV=production CUSTOM_ENV=local next build && npm run transpile:server",
        "start:staging": "NODE_ENV=production CUSTOM_ENV=staging node _next/server/index.js --expose-http2 ",
        "start:production": "NODE_ENV=production CUSTOM_ENV=production node dist/server/index.js --expose-http2 ",
        "start:local": "NODE_ENV=production CUSTOM_ENV=local node dist/server/index.js --expose-http2 ",
        "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
        "pretty": "prettier --write --config .prettierrc.js {src,pages,server}/**/*.{ts,tsx}",
        "format": "npm run lint -- --fix",
        "build:analyze": "ANALYZE=true next build && tsc --project tsconfig.server.json",
        "yes": "graphql-codegen --config codegen.yml",
        "build-docker": "docker build -t next-app .",
        "build-docker-multistage": "docker build --target production -t next-app -f ./Dockerfile.multistage .",
        "run-docker": "docker run --rm -it -p 3000:3000 next-app",
        "mobx": "mobx-devtools"
    },
    "dependencies": {
        "@apollo/react-hooks": "^4.0.0",
        "@apollo/react-ssr": "^4.0.0",
        "@material-ui/core": "^4.11.2",
